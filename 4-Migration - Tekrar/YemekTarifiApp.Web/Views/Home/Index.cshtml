@using YemekTarifiApp.Web.Models.Dtos
@model List<FoodDto>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome YemekTarifiApp</h1>
</div>
<div class="mt-4">
    <div class="row">
        @foreach (var foodDto in Model)
        {
                <div class="col-md-4">
                @await Html.PartialAsync("_FoodPartial", foodDto);
                </div>
        }
    </div>
</div>
