@model List<Proj04_mvc.Controllers.Product>
@{
    ViewData["Title"] = "GetProducts";
}

<h1 class="display-1">All Products</h1>


<ul>
    @foreach (var p in Model)
    {
        <li>@p.Id- @p.Name -@p.Price</li>
    }
</ul>















<ul>
    @foreach (var p in ViewBag.Product)
    {

        <li>@p.Id - @p.Name - @p.Price</li>
    }
</ul>